        <figure class="figure imagewidget__figure" itemscope itemtype="https://schema.org/ImageObject">

                <a href="none" target="none" title="" class="image-link  imagewidget__image-link  js-pswp-image-trigger " itemprop="url">

                        <img class="image  imagewidget__image js-pswp-image" src="//d2f0ora2gkri0g.cloudfront.net/ed/dc/eddc0169-ee36-4d59-997d-2d08c16f2d04.jpg" alt="" data-path="//d2f0ora2gkri0g.cloudfront.net/45/51/4551071b-c82d-4bc6-ae79-636640e7777d.jpg" title="" data-filename="Xiangzhou_of_Zhuozhengyuan_Suzhou_4th_Aug_2006.jpg" data-filesize=""  width= "2304"  itemprop="contentURL">
